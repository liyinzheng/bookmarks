{
  "manifest_version": 2,
  "name": "bookmarks",
  "version": "1.0.0",
  "description": "简单的Chrome扩展-bookmarks",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
  },
  "permissions": ["bookmarks", "storage", "chrome://favicon/"],
  "chrome_url_overrides": {
    "newtab": "./newtab/index.html"
  },
  "homepage_url": "https://liyinzheng.cn"
}
